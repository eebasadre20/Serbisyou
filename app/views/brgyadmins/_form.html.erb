     <div class="row">
        <div class="col-xs-12">
            <div class="row">
                <section class="col-lg-10 col-lg-offset-1 col-md-10 col-md-offset-1 col-xs-12 col-sm-offset-0">
                    <div class="well well-lg">
                        <div class="row">
                             <%= form_for(@admin, :html => {:multipart => true}) do |f| %> 
                            <div class="col-sm-6 col-xs-12">
                            <label>Name</label>
                                <div class="row">
                                    <div class="col-xs-12 col-sm-6 col-md-6">
                                      <div class="form-group">
                                          <%= f.text_field :firstname,class: 'form-control input-md', placeholder: 'Firstname' %>
                                      </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 col-md-6">
                                      <div class="form-group">
                                          <%= f.text_field :lastname,class: 'form-control input-md', placeholder: 'Lastname' %>
                                      </div>
                                    </div>
                                </div>
                                <label>Username</label>
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-12">
                                      <div class="form-group">
                                          <%= f.text_field :username,class: 'form-control input-md', placeholder: 'Username' %>
                                      </div>
                                    </div>
                                </div> 
                              <label>Email</label>
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-12">
                                      <div class="form-group">
                                          <%= f.email_field :email, class: 'form-control input-md', placeholder: 'Email' %>
                                      </div>
                                    </div>
                                </div> 
                              <label>Password</label>  
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-12">
                                      <div class="form-group"> 
                                          <%= f.password_field :password, class: 'form-control input-md', placeholder: 'Password' %>
                                      </div>
                                    </div>
                                </div> 
                              <label>Password Confirmation</label> 
                                <div class="row">
                                    <div class="col-xs-12">
                                      <div class="form-group">  
                                          <%= f.password_field :password_confirmation,  class: 'form-control input-md',placeholder: 'Password Confirmation' %>
                                      </div>
                                    </div>
                                </div>   
                                <label>Birth Date</label>
                                <div class="row">
                                    <div class="col-xs-12">
                                          <%= f.date_select :birthdate, :start_year => Date.current.year, :end_year => 1920 %>
                                      </div>
                                    </div>
                                      <label>Gender</label>
                                      <div class="row">
                                        <div class="col-md-12">
                                          <div class="form-group">
                                                <%= f.select :gender, %w{ Male Female } %>
                                          </div>
                                        </div>
                                      </div>

                                      <label>Barangay</label>
                                      <div class="row">
                                          <div class="col-xs-12 col-sm-12 col-md-12">
                                            <div class="form-group">
                                            <%= f.text_field :barangay, class: 'form-control input-md', placeholder: 'Barangay' %>
                                            </div>
                                          </div>
                                      </div> 

                                      <label>Barangay Position</label>
                                      <div class="row">
                                          <div class="col-xs-12 col-sm-12 col-md-12">
                                            <div class="form-group">
                                            <%= f.text_field :brgy_position,class: 'form-control input-md', placeholder: 'Barangay Position' %>
                                            </div>
                                          </div>
                                      </div> 

                              
                          </div>
                            <div class="col-sm-6 col-xs-12">
                                <div class="row">
                                    <div class="col-xs-12">

                                    </div>
                                </div>
                                    
                                    <label>Contact Number</label>
                                      <div class="row">
                                          <div class="col-xs-12">
                                             <div class="form-group">                                 
                                                <%= f.text_field :contact_number, class: 'form-control input-md', placeholder: 'Contact Number' %>
                                              </div>
                                          </div>
                                      </div>
                                    
                                    <center><label>Profile Picture</label></center>
                                    <div class="row">
                                      <div class="col-xs-12">
                                        <center><div class="fileinput fileinput-new" data-provides="fileinput">
                                          <div class="fileinput-preview thumbnail" data-trigger="fileinput" style="width: 200px; height: 150px;"></div>
                                          <div>
                                            <span class="btn btn-primary btn-file"><span class="fileinput-new">Select image</span><span class="fileinput-exists">Change</span><%= f.file_field :avatar %></span>
                                            <a href="#" class="btn btn-danger fileinput-exists" data-dismiss="fileinput">Remove</a>
                                          </div>
                                        </div></center>
                                    </div><!-- end of column -->
                                </div><!-- end of row -->
                          </br></br></br><hr/>
                              <div class="form-group">
                                <%= f.submit("Register", class: 'btn btn-lg btn-success col-xs-12') %>
                              </div>
                            </div><!-- end of column 2 -->
                        </div><!-- end of well row -->
                        <% end %>
                    </div><!-- end of well -->
                </section>
            </div><!-- end of row -->
        </div>
    </div> 